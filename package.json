{
  "name": "better-photos",
  "version": "1.0.0",
  "description": "The HTML & CSS for a photo sharing app",
  "main": "index.js",
  "scripts": {
    "compileSass": "npx sass --watch --no-source-map src/main.scss src/style.css",
    "build:compile": "sass --no-source-map src/main.scss build/compiled.css",
    "build:prefix": "postcss build/compiled.css --use autoprefixer --output build/prefixed.css",
    "build:optimize": "csso build/prefixed.css --output build/style.css"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^10.4.19",
    "csso-cli": "^4.0.2",
    "postcss": "^8.4.38",
    "postcss-cli": "^11.0.0",
    "sass": "^1.75.0"
  }
}
